{
  "name": "dnd-test",
  "version": "1.0.0",
  "description": "test react-dnd",
  "scripts": {
    "lint": "eslint --ext jsx src/app",
    "dev": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 3000 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors > ret.log",
    "start": "supervisor -w ./src/server/ ./src/server/server.js",
    "dev-all": "concurrently --kill-others \"npm run dev\" \"npm run start\"",
    "build-prod": "webpack --config webpack-production.config.js --progress --profile --colors > ret-prod.log",
    "build-test": "webpack --config webpack-test.config.js --progress --profile --colors > ret-test.log",
    "build-dev": "webpack --config webpack-dev.config.js --progress --profile --colors > ret-dev.log",
    "build-dll": "webpack --config make-webpack-dll.js",
    "build-demo-dll": "webpack --config ./src/demo/make-webpack-dll.js",
    "demo": "webpack-dev-server --progress --colors --hot --inline --config ./src/demo/webpack-hot-dev.config.js",
    "build-demo": "npm run build-demo-dll && webpack --config ./src/demo/webpack-production.config.js --json > ./build/demo/stats.json"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-airbnb": "^1.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "hapi": "12.1.0",
    "html-loader": "0.4.0",
    "json-loader": "^0.5.4",
    "less": "2.5.3",
    "less-loader": "^2.2.3",
    "null-loader": "0.1.1",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss": "^5.0.21",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.6.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "0.8.0",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "15.3.0",
    "react-hot-loader": "1.3.0",
    "react-proxy-loader": "0.3.4",
    "request": "2.67.0",
    "style-loader": "0.13.0",
    "supervisor": "0.9.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.12",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "immutable": "3.7.6",
    "keymirror": "0.1.1",
    "lodash": "3.10.1",
    "moment": "2.11.1",
    "normalize.css": "3.0.3",
    "object-assign": "^4.1.0",
    "react": "15.3.0",
    "react-addons-css-transition-group": "15.3.0",
    "react-addons-linked-state-mixin": "15.3.0",
    "react-addons-pure-render-mixin": "15.3.0",
    "react-addons-shallow-compare": "15.3.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-docgen": "^2.9.1",
    "react-dom": "15.3.0",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "superagent": "1.4.0"
  }
}
